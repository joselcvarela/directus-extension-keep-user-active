{
  "name": "directus-extension-keep-user-active",
  "version": "1.0.0",
  "description": "Directus App extension to keep user active by refreshing session every 5 minutes",
  "icon": "extension",
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "src/index.js",
    "source": "src/index.js",
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "12.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joselcvarela/directus-extension-keep-user-active"
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-hook",
    "extension",
    "app",
    "editor",
    "refresh",
    "auth",
    "session",
    "active"
  ],
